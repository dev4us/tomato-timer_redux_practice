{
  "name": "expo",
  "version": "29.0.0",
  "description": "The Expo SDK",
  "main": "src/Expo.js",
  "bin": {
    "expo": "bin/cli.js"
  },
  "files": [
    "bin",
    "src",
    "flow",
    "tools",
    "AppEntry.js"
  ],
  "scripts": {
    "lint": "eslint src tools",
    "prepublishOnly": "proofread",
    "test": "jest --watch",
    "ci": "yarn run lint && yarn flow check && yarn jest --ci --maxWorkers 2"
  },
  "jest": {
    "preset": "jest-expo",
    "clearMocks": true,
    "testRegex": "/__tests__/.*-test\\.js$",
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/node_modules/jest-expo/src/setup.js",
      "<rootDir>/test/setup.js"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": "universe/native",
    "overrides": {
      "files": "**/__tests__/*.js",
      "env": {
        "node": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-sdk.git"
  },
  "keywords": [
    "expo"
  ],
  "author": "Expo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/expo-sdk/issues"
  },
  "homepage": "https://github.com/expo/expo-sdk",
  "dependencies": {
    "expo-asset": "^1.0.0",
    "expo-camera": "^1.0.0",
    "expo-constants": "^1.0.0",
    "expo-core": "^1.0.0",
    "expo-gl": "^1.0.0",
    "expo-face-detector": "^1.0.0",
    "expo-file-system": "^1.0.0",
    "expo-permissions": "^1.0.0",
    "expo-react-native-adapter": "^1.0.0",
    "expo-sensors": "^1.0.0",
    "expo-sms": "^1.0.0",
    "@expo/vector-icons": "^6.3.1",
    "@expo/websql": "^1.0.1",
    "babel-preset-expo": "^4.0.0",
    "fbemitter": "^2.1.1",
    "invariant": "^2.2.2",
    "lodash.map": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.zipobject": "^4.1.3",
    "lottie-react-native": "2.5.0",
    "md5-file": "^3.2.3",
    "pretty-format": "^21.2.1",
    "prop-types": "^15.6.0",
    "qs": "^6.5.0",
    "react-native-branch": "2.2.5",
    "react-native-gesture-handler": "1.0.6",
    "react-native-maps": "0.21.0",
    "react-native-reanimated": "1.0.0-alpha.3",
    "react-native-svg": "6.2.2",
    "uuid-js": "^0.7.5"
  },
  "devDependencies": {
    "@expo/npm-proofread": "^1.0.1",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.10.0",
    "eslint-config-universe": "^1.0.7",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.77.0",
    "jest-expo": "^27.0.1",
    "prettier": "^1.7.4",
    "react": "^16.3.1",
    "react-native": "expo/react-native#sdk-29",
    "react-test-renderer": "^16.3.1"
  }
}
